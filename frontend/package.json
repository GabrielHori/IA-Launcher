{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tailwind:init": "tailwindcss init -p",
    "tailwind:build": "tailwindcss -i ./src/index.css -o ./dist/output.css --minify",
    "start-backend": "start cmd /k \"cd ../backend && .venv\\Scripts\\activate && python -m uvicorn app.main:app --reload --port 11451\"",
    "tauri:dev": "npm run start-backend && tauri dev",
    "tauri:build": "npm run build && tauri build"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
