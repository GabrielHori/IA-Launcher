{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
     "dev": "concurrently \"npm:tauri\" \"npm:backend\" --names \"Backend,Frontend\" --prefix-colors \"magenta,blue\"",
    "tauri": "npx tauri dev",
    "backend": "cd ../backend && .venv\\Scripts\\python.exe -m uvicorn app.main:app --host 127.0.0.1 --port 11451 --reload",
    "ollama": "ollama serve",
    "build": "npm run build",
    "preview": "vite preview",
    "tailwind:init": "tailwindcss init -p",
    "tailwind:build": "tailwindcss -i ./src/index.css -o ./dist/output.css --minify",
    "start-backend": "start cmd /k \"cd ../backend && .venv\\Scripts\\activate && python -m uvicorn app.main:app --reload --port 11451\""
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tauri-apps/cli": "^2.9.6",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
